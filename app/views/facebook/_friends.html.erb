<div class='photoSelectorFriends friendSelector'>
  <label>Select a friend</label>
  <table>
    <% @friends.each do |friend| %>
      <tr class='friend' data-fb_uid='<%= friend['id'] %>'>
        <td><%= link_to(image_tag("https://graph.facebook.com/#{friend['id']}/picture"), '') %></td>
        <td><%= link_to(friend['name'], '') %></td>
      </tr>
    <% end %>
  </table>
</div>
