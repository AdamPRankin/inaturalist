Feeds for Current Filters:
<%- 
  feed_params = params.reject do |key,value|
    ObservationsController::REJECTED_FEED_PARAMS.include?(key)
  end
  kml_feed_params = params.reject do |key,value|
    ObservationsController::REJECTED_KML_FEED_PARAMS.include?(key)
  end
  kml_feed_params[:kml_type] = "network_link"
-%>
<%= link_to("Atom", url_for(feed_params.merge(:format => 'atom')), :id => 'atomlink') %>
<%= link_to("KML",  url_for(kml_feed_params.merge(:format => 'kml')), :id => 'kmllink', :rel => "nofollow") %>
<%= link_to("CSV",  url_for(feed_params.merge(:format => 'csv')), :id => 'csvlink', :rel => "nofollow") %>